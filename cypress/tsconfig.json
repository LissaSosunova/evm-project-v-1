{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "outDir": "./outTests",
        "strict": true,
        "baseUrl": "../node_modules",
        "target": "es5",
        "lib": ["es5", "dom", "es2015.promise", "es2016", "es2017"],
        "types": ["cypress"],
        "module": "commonjs",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "noImplicitAny": false,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "alwaysStrict": true
    },
    "include": ["**/*.ts", "typings.d.ts"]
}
